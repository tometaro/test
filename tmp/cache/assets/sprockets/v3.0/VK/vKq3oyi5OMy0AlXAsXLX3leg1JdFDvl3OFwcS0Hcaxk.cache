# http://www.appveyor.com/docs/lang/nodejs-iojs

environment:
  matrix:
    # node.js
    - nodejs_version: "0.10"
    - nodejs_version: "0.12"
    - nodejs_version: "4"
    - nodejs_version: "5"
    - nodejs_version: "6"

install:
  - IF %nodejs_version% EQU 0.10 npm -g install npm@2
  - IF %nodejs_version% EQU 0.10 set PATH=%APPDATA%\npm;%PATH%
  - ps: Install-Product node $env:nodejs_version
  - npm install

test_script:
  - node --version
  - npm --version
  # power shell
  - ps: "npm test"
  # standard command line
  - cmd: npm test

build: off

version: "{build}"
